<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" style="-fx-background-color: beige;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sir.choosepage.ChooseServerController">
   <children>
      <VBox id="vBox" alignment="CENTER" layoutY="200.0" maxHeight="200.0" prefHeight="200.0" prefWidth="600.0" spacing="5.0" style="-fx-background-color: beige;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button id="mysqlButton" fx:id="mysqlButton" mnemonicParsing="false" onAction="#getMySqlConn" prefWidth="200.0" text="MySQL Server">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button id="posButton" fx:id="posgreButton" alignment="CENTER" mnemonicParsing="false" onAction="#getPosGreConn" prefWidth="200.0" text="PostgreSQL Server" />
            <Button fx:id="oracleButton" mnemonicParsing="false" prefWidth="200.0" text="Oracle Server" />
            <Button fx:id="microsoftButton" mnemonicParsing="false" prefWidth="200.0" text="MicrosoftSQL Server" />
            <Button fx:id="mongoButton" mnemonicParsing="false" prefWidth="200.0" text="Mongo Server" />
         </children>
      </VBox>
   </children>
</AnchorPane>
